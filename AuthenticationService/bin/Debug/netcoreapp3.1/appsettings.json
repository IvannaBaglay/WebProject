{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [ "WithMachineName", "WithProcessId", "WithThreadId", "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "../logs/AuthenticationService/RuntimeLogs.txt"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://host.docker.internal:9200",
          "indexFormat": "Serilog-AuthService-{0:yyyy.MM.dd}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AuthenticationServiceContext": "Server=host.docker.internal;Port=3306;database=users_schema;user=user;password=password;",
    "Ubiservices": "https://uat-public-ubiservices.ubi.com/v3/profiles/sessions"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:42420"
      }
    }
  }
}